inicio:
yield
#leer la clase de una tolva, sin utilidad
#ls r0 d5 0 SortingClass
#s db Setting r0
#si cabiaron la maquina es que hay que vaciar el apilador
lbn r1 HASH("StructureLogicMemory") HASH("maquina") Setting Minimum
bnezal r1 apilador
j inicio

apilador:
sbn HASH("StructureStacker") HASH("Apilador terminados") Activate 1
yield
sbn HASH("StructureStacker") HASH("Apilador terminados") Activate 0
j ra
